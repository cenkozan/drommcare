  "author": "Cenk Ozan Kahraman",
  "description": "We are starting with a basic caretaker planning and billing software",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "start": "node dist/server/app.js",
    "predev": "tsc -p server",
    "dev": "concurrently \"mongod\" \"http-server client -p 8080 --cors\" \"nodemon dist/server/app.js\"",
    "prod": "concurrently \"mongod\" \"http-server client -p 8080 --cors && tsc -p server && node dist/server/app.js\"",
    "test": "ng test",
    "testbe": "tsc -p server && node_modules/.bin/mocha dist/server/test",
    "lint": "ng lint",
    "lintbe": "tslint server/**/**.ts{,x}",
    "e2e": "ng e2e"
  },
  "private": true,
